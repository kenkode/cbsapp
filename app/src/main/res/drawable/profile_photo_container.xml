<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/colorGrayAccent"/>
            <solid android:color="@color/colorAccent"/>
        </shape>
    </item>
    <item
        android:bottom="1dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/colorGrayAccent"/>
        </shape>

    </item>

</layer-list>